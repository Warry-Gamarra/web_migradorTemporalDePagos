@model IEnumerable<WebMigradorCtasPorCobrar.Models.Helpers.Response>

@foreach (var response in Model)
{
    <div class="card-body">
        <h5 class="card-title">Resultado: @response.CurrentID</h5>
        <div class="card-text alert alert-@response.Color">@Html.Raw(response.Message)</div>
    </div>
}
