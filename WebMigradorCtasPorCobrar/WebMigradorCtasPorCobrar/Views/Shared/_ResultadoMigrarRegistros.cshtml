@model IEnumerable<WebMigradorCtasPorCobrar.Models.Helpers.Response>

@foreach (var result in Model)
{
    <div class="card-body">
        <h5 class="card-title">Resultado: @result.CurrentID</h5>
        <div class="card-text alert alert-@result.Color">@Html.Raw(result.Message)</div>
    </div>

}

