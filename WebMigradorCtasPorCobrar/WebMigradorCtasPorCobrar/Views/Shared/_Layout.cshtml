<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Migrador Temporal de pagos</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("UNFV - Migrador Temporal de pagos", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav pull-right">
                    <li>@Html.ActionLink("PREGRADO", "Index", "Pregrado")</li>
                    <li>@Html.ActionLink("POSGRADO", "Index", "Posgrado")</li>
                    <li>@Html.ActionLink("EUDED", "Index", "Euded")</li>
                    <li>@Html.ActionLink("TASAS", "Index", "Tasas")</li>
                    <li>@Html.ActionLink("CAJA PERIFERICA", "Index", "CajaPeriferica")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container-fluid body-content" style="padding-top: 50px;">
        @Html.Partial("_Tablas")
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - UNFV</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
